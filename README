details
	index 		/project/{id}/detail 					GET
	create 		/project/{id}/detail 					POST
	show 		/project/{id}/detail/{detailKey} 		GET
	update		/project/{id}/detail					PUT
	destroy		/project/{id}/detail					DELETE
	update 		/project/{id}/detail/{detailKey}/edit 	POST
	delete 		/project/{id}/detail/{detailKey}/delete GET
	destroy 	/project/{id}/detail/{detailKey}/delete POST

