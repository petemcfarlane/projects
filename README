Projects

• Task orientated
• Log meetings
• List details

Routes

projects
	index 		/ 						GET
	create 		/ 						POST
	show 		/project/{id} 			GET
	update 		/project/{id}	 		POST
	destroy 	/project/{id}/delete 	POST

detail
	index 		/project/{id}/detail 					GET
	create 		/project/{id}/detail 					POST
	update 		/project/{id}/detail/{detailId}		 	POST
	destroy 	/project/{id}/detail/{detailId}/delete	POST

notes
	index		/project/{id}/notes					GET
	create		/project/{id}/notes					POST
	newNote		/project/{id}/notes/new				GET
	show		/project/{id}/notes/{noteId}		GET
	update		/project/{id}/notes/{noteId}		POST
	destroy		/project/{id}/notes/{noteId}/delete	POST

tasks
	index		/project/{id}/tasks					GET
	create		/project/{id}/tasks					POST
	update		/project/{id}/tasks/{taskId}		POST
	destroy		/project/{id}/tasks/{taskId}/delete	POST




meetings
	index
	create
	show
	update
	delete
	destroy